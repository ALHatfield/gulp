{{#if production}}
    <script>
        window.onload = function() {
            if (Enabler.isInitialized()) {
                enablerInitHandler();
            } else {
                Enabler.addEventListener(studio.events.StudioEvent.INIT, enablerInitHandler);
            }
        };

        function enablerInitHandler() {
            // Start ad, initialize animation,
            // load in your image assets, call Enabler methods,
            // and/or include other Studio modules.
            // Also, you can start the Polite Load

            var extCSS=document.createElement('link');
            extCSS.setAttribute("rel", "stylesheet");
            extCSS.setAttribute("type", "text/css");
            extCSS.setAttribute("href", "%%%% fontFamilyCDN %%%%");
            document.getElementsByTagName("head")[0].appendChild(extCSS);

            extCSS=document.createElement('link');
            extCSS.setAttribute("rel", "stylesheet");
            extCSS.setAttribute("type", "text/css");
            extCSS.setAttribute("href", Enabler.getUrl("combined.min.css"));
            document.getElementsByTagName("head")[0].appendChild(extCSS);

            var extJavascript = document.createElement('script');
            extJavascript.setAttribute('type', 'text/javascript');
            extJavascript.setAttribute('src', Enabler.getUrl('combined.min.js'));
            document.getElementsByTagName('head')[0].appendChild(extJavascript);
        }
    </script>
